<style>

    .regionbtn {
        box-shadow: none !important;
        border-color: #0dcaf0 !important;
        color: #0dcaf0 !important;
    }

    .iti {
        width: 100%;
    }

    .iti__country-list {
        z-index: 3 !important;
    }

    .form_doc {
        position: absolute;
        width: 80px;
        margin-top: 4px;
        opacity: 0;
    }

    .form_doc_mobile {
        position: absolute;
