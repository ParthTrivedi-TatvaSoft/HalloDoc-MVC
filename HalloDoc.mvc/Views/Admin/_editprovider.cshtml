<div class="container">
    <div class=" mt-2 d-flex justify-content-between align-items-center">
        <h4>My Profile</h4>
        <button onclick="providers()"
                class="btn shadow  btn-outline-secondary border-info text-info px-3 ">
            <i class="fa-solid fa-angle-left fs-6"></i> Back
        </button>
    </div>
    @*  <form action="" class="shadow border rounded my-3" method="post"> *@
    <div class="shadow border rounded my-3">
        <div class="mx-3 my-4 ">
            <form action=" " method="post" id="ResetPasswordForm">
                <div class="mt-3">
                    <h4 class="my-4 pt-4">Account Information</h4>

                    <div class="row">
                        <div class="mb-3 col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating  ">
                            <input type="text" class="form-control " id="floatingInputUsername" placeholder="User Name" disabled aria-label="User Name" asp-for="@Model.editPro.username">
                            <label class="ms-2 " for="floatingInputUsername">User Name</label>
                        </div>
                        <div class="mb-3 col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating ">
                            <input type="password" class="form-control bg-transparent"
                                   id="floatingPassword2" placeholder="Password">
                            <label class="ms-2" for="floatingPassword2">Password</label>
                            <div class="eye-icon position-absolute top-0 end-0 mx-3 my-2">
                                <i class="bi bi-eye-slash fs-4 text-muted mt-1 common-cursor-pointer" id="togglePassword2" onclick="showPassword()"></i>
                            </div>

                        </div>
                        @*  <input asp-for="@Model.AdminProfile.Aspnetuserid" id="hidden1" hidden /> *@
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12  form-floating mb-3 ">
                            <select class="form-select form-control d1class" disabled id="status">
                                <option value="1">Active</option>
                                <option value="2">Pending</option>
                                <option value="3">Not Active</option>
                            </select>
                            <label for="status" class="ms-2 ">Status</label>
                        </div>
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12  form-floating mb-3 ">
                            <select class="form-select form-control d1class" disabled id="Role">
                                <option value="1">Master Admin</option>
                                <option value="2">Physician</option>
                                <option value="3">Provider</option>
                            </select>

                            <label for="Role" class="ms-2 ">Role</label>
                        </div>
                    </div>
                    <div class="d-flex justify-content-end mt-3">
                        <button type="button" id="editBtn1" class="btn btn-info text-white EditBtn"
                                onclick="enableFields(1)">
                            Edit
                        </button>

                        <button type="submit" id="submitBtn1" class="btn btn-info text-white SaveBtn me-2">Save</button>

                        <button type="button" id="cancelBtn1" class="btn btn-outline-info CancelBtn"
                                onclick="disableFields(1)">
                            Cancel
                        </button>
                        <button type="submit" id="ResetPassword"
                                class="btn btn-outline-secondary border-info text-info ms-2">
                            Reset Password
                        </button>
                    </div>
                </div>
            </form>

            <form action=" " method="post" id="AdministratorForm">
                <div class="pt-4">
                    <h4 class="mb-4">Physician Information</h4>
                    <div class="row mb-3">
                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3 ">
                            <input type="text" class="d2class form-control " id="floatingInputFirstName" asp-for="@Model.editPro.Firstname"
                                   placeholder="First Name" aria-label="First Name">
                            <label class="ms-2 " for="floatingInputFirstName">First Name</label>
                        </div>
                        @* <input asp-for="@Model.AdminProfile.AdminId" id="hidden2" hidden /> *@
                        @* <input asp-for="@Model.AdminProfile.Aspnetuserid" id="hidden3" hidden /> *@
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12  form-floating mb-3 ">
                            <input type="text" class="d2class form-control " id="floatingInputLast" asp-for="@Model.editPro.Lastname"
                                   placeholder="Last Name" aria-label="Last Name">
                            <label class="ms-2  " for="floatingInputLast">Last Name</label>
                        </div>
                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input type="email" class="d2class form-control " id="ProfileInputemail" asp-for="@Model.editPro.Email"
                                   placeholder="Email" aria-label="Email">

                            <label class="ms-2 " for="floatingInputemail">Email</label>
                        </div>
                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input style="height:58px;" id="providerMobile" asp-for="@Model.editPro.PhoneNumber"
                                   class="form-control contact d2class"
                                   aria-label="Contact"
                                   type="number" />
                            @* <span asp-validation-for="ff_phone" class="text-danger"></span> *@
                        </div>
                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input type="text" class="d2class form-control " id="MedicalLicense" asp-for="@Model.editPro.MedicalLicesnse"
                                   placeholder="MedicalLicense" aria-label="MedicalLicense">
                            <span class="text-danger"></span>
                            <label class="ms-2 " for="MedicalLicense">MedicalLicense #</label>
                        </div>
                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input type="text" class="d2class form-control " id="NPINumber" asp-for="@Model.editPro.NPInumber"
                                   placeholder="NPINumber" aria-label="NPINumber">
                            <span class="text-danger"></span>
                            <label class="ms-2 " for="NPINumber">NPI Number</label>
                        </div>
                        <div class=" col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <input type="email" class="d2class form-control " id="SynchronizationEmail" asp-for="@Model..SycnEmail"
                                   placeholder="SynchronizationEmail" aria-label="SynchronizationEmail">
                            <span class="text-danger"></span>
                            <label class="ms-2 " for="SynchronizationEmail">Synchronization Email Address</label>
                        </div>
                        <div class="d-flex col-xl-6 col-lg-6 col-md-6 col-sm-12 form-floating mb-3">
                            <button class="btn border d-flex align-items-center w-100 regionbtn d2class" type="button" style="height:58px" data-bs-toggle="dropdown" aria-expanded="false">
                                Select Region
                            </button>
                            <div class="dropdown-menu d1class">
                                @if (Model.physicianregiontable is not null && Model.physicianregiontable.Count > 0)
                                {
                                    @foreach (var region in Model.physicianregiontable)
                                    {
                                        <div class="dropdown-item">
                                            <input type="checkbox" @(region.ExistsInTable ? "checked" : "") name="regions" id="@region.Regionid" value="@region.Regionid" class="region-checkbox me-1">
                                            <label for="@region.Regionid">@region</label>
                                        </div>
                                    }
                                }
                            </div>
                        </div>
                    </div>
                    <div class="d-grid gap-2 d-flex justify-content-end  justify-content-md-end mt-3 mb-3">
                        <button type="button" id="editBtn2" class="btn btn-info text-white EditBtn"
                                onclick="enableFields(2)">
                            Edit
                        </button>

                        <button type="submit" id="submitBtn2" class="btn btn-info text-white SaveBtn ">Save</button>

                        <button type="button" id="cancelBtn2" class="btn btn-outline-info CancelBtn "
                                onclick="disableFields(2)">
                            Cancel
                        </button>
                    </div>
                </div>
            </form>

