<script>

    $(document).ready(function () {
        $('.Onboarding').change(function () {
            var checkboxId = $(this).attr('id');
            var buttonId = $(this).attr('value');

            if ($(this).is(':checked')) {
                $('#OnboardingView' + buttonId).show();
            } else {
                $('#OnboardingView' + buttonId).hide();
            }
        });
    });


    function disableFields(formid) {
        document.querySelectorAll('.d' + formid + 'class').forEach(function (element) {
            element.disabled = true;
        });
        // Hide the submit and cancel buttons, show the edit button
        document.getElementById('submitBtn' + formid).style.display = 'none';
        document.getElementById('cancelBtn' + formid).style.display = 'none';
        document.getElementById('editBtn' + formid).style.display = 'block';
    }

    function enableFields(formid) {
        document.querySelectorAll('.d' + formid + 'class').forEach(function (element) {
            element.disabled = false;
        });
        // Show the submit and cancel buttons, hide the edit button
        document.getElementById('submitBtn' + formid).style.display = 'block';
        document.getElementById('cancelBtn' + formid).style.display = 'block';
        document.getElementById('editBtn' + formid).style.display = 'none';
    }
    disableFields(1);
    disableFields(2);
    disableFields(3);
    disableFields(4);

    function showPassword() {

        const togglePassword = document.querySelector("#togglePassword2");
        const password = document.querySelector("#floatingPassword2");

        const type = password.getAttribute("type") === "password" ? "text" : "password";
        password.setAttribute("type", type);

        this.classList.toggle('bi-eye');
        this.classList.toggle('bi-eye-slash');
    }

    $(document).ready(function () {
        $('#inputGroupFile').on('change', function () {
            if (checkForm() == true) {
                var inputValue = $('#inputGroupFile').val();

                if (inputValue != "") {
                    $("#upload-admin-btn").removeClass("d-none");
                }
                else {
