@using DataAccess.CustomModels;
@model BlockHistory2;<link rel="stylesheet" href="https://cdn.datatables.net/1.13.7/css/dataTables.bootstrap5.min.css" /><div class="w-100 px-2 px-sm-5">    <h3 class="ms-2 my-1">Blocked History</h3>    <div class="shadow rounded-2 bg-white">        <form id="blockHistoryForm">            <div class="row g-2 my-2 p-3 mb-4">                <div class="form-floating mb-3 col-sm-6 col-md-3 col-lg-3 col-xl-3 d-flex">                    <input type="text" class="form-control bg-transparent" id="first_name" placeholder="First Name" asp-for="searchRecordOne">                    <label for="first_name" class="ms-2 text-muted">Name</label>                </div>                <div class="form-floating mb-3 col-sm-6 col-md-3 col-lg-3 col-xl-3 d-flex">                    <input type="date" class="form-control bg-transparent" id="dateBlockHistory" placeholder="To Date of Service" asp-for="searchRecordTwo">                    <label for="dateBlockHistory" class="ms-2 text-muted">Date</label>                </div>                <div class="form-floating mb-3 col-sm-6 col-md-3 col-lg-3 col-xl-3 d-flex">                    <input type="email" class="form-control " id="email" placeholder="Email" aria-label="Email"  asp-for="searchRecordThree">                    <label class="ms-2 text-muted" for="Email">Email</label>                </div>                <div class="form-floating mb-3 col-sm-6 col-md-3 col-lg-3 col-xl-3 d-flex">                    <input class="form-control" type="search" placeholder="Phone Number" aria-label="Search" id="phone" asp-for="searchRecordFour">                    <label class="text-muted ms-2">Phone Number</label>                </div>                <div class="d-flex justify-content-end">
                    <button class="btn btn-outline-info me-2 px-3 py-2" type="button" id="clear_button" onclick="clearAllBlockHistory()">Clear</button>                    <button class="btn btn-info text-white ms-1 px-3 py-2" onclick="blockHistorySearch()" type="button" id="search_button">Search</button>                </div>            </div>        </form>                  <table class="table" id="blcokHistoryTable">                <thead class="table-active">                    <tr>                        <th scope="col" class="text-center">Patient Name</th>                        <th scope="col" class="text-center">Phone Number </th>                        <th scope="col" class="text-center">Email</th>                        <th scope="col" class="text-center">Created Date<i class="bi bi-arrow-down"></i></th>                        <th scope="col" class="text-center">Notes</th>                        <th scope="col" class="text-center">Is Active</th>                        <th scope="col">Actions</th>                    </tr>                </thead>                <tbody>                @if(Model.blockHistories!=null && Model.blockHistories.Count>0){
                    
                    @foreach (var obj in Model.blockHistories)                    {                        <tr>                            <td class="text-center">@obj.patientname</td>                            <td class="text-center">@obj.phonenumber</td>                            <td class="text-center">@obj.email</td>                            <td class="text-center">@obj.createddate</td>                            <td class="text-center">@obj.notes</td>                            <td class="text-center"><input type="checkbox" name="Mark" class="checkbox fileCheckbox px-4 providerCheckBox" checked onchange="checkBoxBlock(@obj.blockId)" /></td>                            <td><button class="btn btn-outline-info rounded-2" onclick="unblockBlockHistory(@obj.blockId)">Unblock</button></td>                        </tr>                    }
                }
                else
                {
                    <tr>
                        <td colspan="7" style="text-align:center;">No Request Found</td>
                    </tr>
                }            </tbody>            </table>                 </div>    <div class="border rounded shadow d-block d-lg-none">        <div class="accordion container-fluid p-0" id="accordionPanelsStayOpenExample">            <table class="w-100 requesttable">               @*  <thead>                    <tr>                        <th class="d-none"></th>                        <th class="d-none"></th>                    </tr>                </thead>                @{                    var counter = 1;                }                <tbody>                    @if (Model.getRecordExplore != null)                    {                        @foreach (var obj in Model.getRecordExplore)                        {                            <tr class="d-flex flex-column">                                <td>                                    <h2 class="accordion-header" id="flush-heading@(counter)"></h2>                                    <div class="accordion-button collapsed d-flex flex-column pb-0" data-bs-toggle="collapse" data-bs-target="#flush-collapse@(counter)" aria-expanded="false" aria-controls="flush-collapse@(counter)">                                        <div class="w-100 ">                                            <span class="fw-bolder">                                                @obj.fullname                                            </span>                                        </div>                                        <div class="w-100 mt-2">                                            <div class="">                                                @obj.confirmationnumber                                            </div>                                        </div>                                    </div>                                </td>                                <td>                                    <div id="flush-collapse@(counter)" class="accordion-collapse collapse" aria-labelledby="flush-heading@(counter)" data-bs-parent="#accordionFlushExample">                                        <div class="accordion-body main-flex pt-2">                                            <div class="w-100">                                                <div class="">                                                    <span class="fw-bolder">Created Date: </span> @obj.createddate                                                </div>                                                <div class="">                                                    <span class="fw-bolder">Provider: </span> Provider Name                                                </div>                                                <div class="">                                                    <span class="fw-bolder">Conclude Date: </span> Conclude Date                                                </div>                                                <div class="">                                                    <span class="fw-bolder">Status: </span> @obj.status                                                </div>                                                <div class="inner-flex d-flex justify-content-end flex-column">                                                    <button style="cursor : pointer" class=" mx-3 mt-1 btn btn-outline-info rounded-2" aria-expanded="false" onclick="viewCaseRecords(@obj.requestid,@obj.status,1)">                                                        View Case                                                    </button>                                                    <button style="cursor : pointer" class="m-3 btn btn-outline-info rounded-2" aria-expanded="false" onclick="viewUploadsRecords(@obj.requestid,1)">                                                        Documents                                                    </button>                                                </div>                                            </div>                                        </div>                                </td>                            </tr>                            counter++;                        }                    }                </tbody> *@            </table>        </div>    </div></div><script>    $("#dateBlockHistory").val(" ")    function checkBoxBlock(blockId) {        $.ajax({            url: "/Admin/BlockHistoryCheckBox",            data: { blockId: blockId },            type: 'GET',            success: function (result) {                if (result.isActive) {                    toastr.success("Active");                } else {                    toastr.success("Not Active");                }            },            error: function () {                alert("Error");            },        });    }    function clearAllBlockHistory() {        let a = document.getElementsByTagName("input")        for (let i = 0; i < a.length; i++) {            a[i].value = "";        }        let b = document.getElementsByTagName("textarea")        for (let i = 0; i < b.length; i++) {            b[i].value = "";        }
        blockHistorySearch();    }    function unblockBlockHistory(blockId){        $.ajax({
            url: "/Admin/unblockBlockHistory",            data: { blockId: blockId },            type: 'POST',            success: function (result) {
                if (result.isUnblocked) {
                    showBlockHistory();
                    toastr.success("Request Unblocked");
                }
                else{                    toastr.error("Something went wrong");
                }             },            error: function () {                alert("Error");            },        });    }    function blockHistorySearch() {        var arrayMain = $("#blockHistoryForm").serializeArray();        console.log(arrayMain)        event.preventDefault();        $.ajax({
            url: "/Admin/BlockedHistory",            data: arrayMain,            type: 'GET',            success: function (result) {
                $("#record-tab-pane").html(result);            },            error: function () {                alert("Error");            },        });    }    // $(document).ready(function () {    //     $('#blcokHistoryTable').DataTable({    //         "initComplete": function (settings, json) {    //             $('#my-search-input').val(settings.oPreviousSearch.sSearch);    //             $('#my-search-input').on('keyup', function () {    //                 var searchValue = $(this).val();    //                 settings.oPreviousSearch.sSearch = searchValue;    //                 settings.oApi._fnReDraw(settings);    //             });    //         },    //         "lengthMenu": [[5, 10, -1], [5, 10, "All"]],    //         "pageLength": 5,    //         // "columns": [{ "searchable": true }, { "searchable": false }, { "searchable": true }, { "searchable": false }, { "searchable": false }, { "searchable": false }, { "searchable": false }, { "searchable": false }, { "searchable": false }],    //         language: {    //             oPaginate: {    //                 sNext: '<i class="bi bi-caret-right-fill text-info"></i>',    //                 sPrevious: '<i class="bi bi-caret-left-fill text-info"></i>'    //             }    //         }    //     });    //     // $('.dataTables_length').hide();    //     $('.dataTables_filter').hide();    // });</script>